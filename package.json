{
  "name": "atom-auto-uglify",
  "main": "./lib/auto-uglify",
  "version": "0.7.2",
  "description": "Minifies a Javascript file on save with UglifyJS",
  "activationCommands": {
    "atom-workspace": [
      "uglify:compile",
      "core:save"
    ]
  },
  "repository": "https://github.com/Metric/Atom-Auto-Uglify",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "uglify-js": ">=2.4.0",
    "uglify-es": ">=3.0.0"
  }
}
